import java.io.*;
import java.util.*;
public class CandidateCode {
    public static void main(String args[] ) throws Exception {

    	Scanner sc = new Scanner(System.in);
    	int n = sc.nextInt();
    	int [] arr = new int[n];
    	int even =0,odd=0;
    	for(int i =0;i<n;i++)
    	{
    	    arr[i]=sc.nextInt();
    	}
    	Arrays.sort(arr);
    	System.out.println(arr[0]*arr[n-1]);
   }
}
